{
    "Version" : "2008-10-17",
    "Id" : "PolicyForCloudFrontPrivateContent",
    "Statement" : [
    {
    "Sid": "AllowCloudFrontServicePrincipal",
    "Effect": "Allow",
    "Principal": {
    "Service": "cloudfront.amazonaws.com"
              },
    "Action": "s3:GetObject",
    "Resource": "${aws_s3_bucket.m-1-1.arn}/*",
    "Condition": {
    "StringEquals": {
    "AWS:SourceArn": "${aws_cloudfront_distribution.s3_distribution.arn}"
                  }
              }
          }
      ]
    }


